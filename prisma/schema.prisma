generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model bengkel_log {
  id           BigInt    @id @default(autoincrement())
  nomor_polisi String?   @db.VarChar(20)
  nama_bengkel String?   @db.VarChar(100)
  jam_masuk    DateTime? @db.Timestamptz(6)
  jam_keluar   DateTime? @db.Timestamptz(6)
  durasi       String?   @db.VarChar(100)
  created_at   DateTime  @default(now()) @db.Timestamptz(6)
}
